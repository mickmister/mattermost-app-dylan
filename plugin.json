{
    "id": "dylan",
    "name": "Dylan",
    "description": "This plugin serves the Dylan app, which allows you to install plugins on this server from a provided PR URL.",
    "homepage_url": "https://github.com/mattermost/mattermost-app-dylan",
    "support_url": "https://github.com/mattermost/mattermost-app-dylan/issues",
    "release_notes_url": "https://github.com/mattermost/mattermost-app-dylan/releases/tag/v0.1.0",
    "icon_path": "assets/starter-template-icon.svg",
    "version": "0.2.0",
    "min_server_version": "5.37.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "settings_schema": {
        "header": "",
        "footer": "",
        "settings": [
            {
                "key": "GitHubAccessToken",
                "display_name": "GitHub Access Token",
                "type": "text",
                "help_text": "Get your token here https://github.com/settings/tokens! You only need the repo:public_repo permission.",
                "default": ""
            }
        ]
    }
}
